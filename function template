#include <iostream>
using namespace std;

// Template function to swap two values
template <typename T>
void swapValues(T &a, T &b) {
    T temp = a;
    a = b;
    b = temp;
}

int main() {
    int x = 10, y = 20;
    cout << "Before swapping: x = " << x << ", y = " << y << endl;
    swapValues(x, y);
    cout << "After swapping: x = " << x << ", y = " << y << endl;

    double m = 1.5, n = 2.5;
    cout << "\nBefore swapping: m = " << m << ", n = " << n << endl;
    swapValues(m, n);
    cout << "After swapping: m = " << m << ", n = " << n << endl;

    return 0;
}
